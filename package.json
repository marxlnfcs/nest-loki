{
  "name": "@marxlnfcs/nest-lokijs",
  "private": false,
  "version": "0.0.1",
  "description": "A LokiJS Module for NestJS",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "upgrade-packages": "node scripts/upgrade-dependencies.js",
    "generate-index": "rimraf ./src/index.ts && barrelsby --config ./barrelsby.config.json",
    "build": "npm run clean && tsc && node scripts/build-package.js",
    "publish": "npm run build && cd dist && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marxlnfcs/nest-lokijs.git"
  },
  "author": "marxlnfcs",
  "bugs": {
    "url": "https://github.com/marxlnfcs/nest-lokijs/issues"
  },
  "homepage": "https://marxlnfcs.github.io/nest-lokijs/",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "barrelsby": "^2.5.1",
    "lodash": "^4.17.21",
    "lokijs": "^1.5.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slug": "^8.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/lokijs": "^1.5.8",
    "@types/node": "^18.16.3",
    "@types/slug": "^5.0.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
